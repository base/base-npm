{
  "name": "base-npm",
  "description": "Base plugin that adds methods for programmatically running npm commands.",
  "version": "0.2.0",
  "homepage": "https://github.com/jonschlinkert/base-npm",
  "author": "Jon Schlinkert (https://github.com/jonschlinkert)",
  "repository": "jonschlinkert/base-npm",
  "bugs": {
    "url": "https://github.com/jonschlinkert/base-npm/issues"
  },
  "license": "MIT",
  "files": [
    "index.js"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=0.10.0"
  },
  "scripts": {
    "test": "mocha"
  },
  "dependencies": {
    "cross-spawn-async": "^2.2.1",
    "extend-shallow": "^2.0.1"
  },
  "devDependencies": {
    "base": "^0.8.1",
    "base-questions": "^0.5.6",
    "delete": "^0.3.0",
    "gulp-format-md": "^0.1.7",
    "mocha": "^2.4.5",
    "pkg-store": "^0.2.1"
  },
  "keywords": [
    "api",
    "cli",
    "commands",
    "dev",
    "flags",
    "install",
    "npm",
    "save",
    "savedev",
    "spawn"
  ],
  "verb": {
    "run": true,
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "highlight": "base-bower",
      "list": [
        "base",
        "base-questions",
        "base-task"
      ]
    },
    "reflinks": [
      "base-questions",
      "verb"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
